{
    "id": "kfjfd",
    "loopCount": 5,
    "usersCount": 1000,
    "rampup": 10,
    "variables": {
        "testein": {
            "site": "portal.testein",
            "domain": "com"
        }
    },
    "steps": [
        {
            "typename": "restrequest",
            "name": "Get all posts",
            "enabled": true,
            "propertyValues": {
                "method": "GET",
                "name": "a",
                "endPoint": "https://jsonplaceholder.typicode.com/posts",
                "headers": {
                    "Content-Type": "application/json; charset=utf-8"
                },
                "variableName": "posts"
            }
        },
        {
            "typename": "comparenumbervalues",
            "name":  "Check status code",
            "enabled": true,
            "propertyValues": {
                "method": "eq",
                "value": "$(posts.StatusCode)",
                "to": "200"
            }
        },
        {
            "typename": "comparevalues",
            "name": "Check title",
            "enabled": true,
            "propertyValues": {
                "value": "$(posts.Body[0].title)",
                "to": "equals|true|sunt aut facere Repellat provident occaecati excepturi optio reprehenderit"
            }
        },
        {
            "typename": "comparevalues",
            "name": "Check title # 2",
            "enabled": true,
            "propertyValues": {
                "value": "$(posts.Body[2].title)",
                "to": "contains|false|ea molestias quasi"
            }
        },
        {
            "typename": "restrequest",
            "name": "Get post",
            "enabled": true,
            "propertyValues": {
                "method": "GET",
                "name": "b",
                "endPoint": "https://jsonplaceholder.typicode.com/posts/$(posts.Body[1].id)",     
                "variableName": "post",
                "headers": {
                    "Content-Type": "application/json; charset=utf-8"
                }           
            }
        },
        {
            "typename": "comparenumbervalues",
            "name":  "Check status code",
            "enabled": true,
            "propertyValues": {
                "method": "eq",
                "value": "$(post.StatusCode)",
                "to": "200"
            }
        },
        {
            "typename": "comparevariable",
            "name": "Check title",
            "enabled": true,
            "propertyValues": {
                "variableName": "post.Body.title",
                "to": "startswith|true|Qui est esse"
            }
        }
    ]
}